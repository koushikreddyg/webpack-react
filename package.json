{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "live": "live-server public",
    "lint": "eslint src/**/*.js",
    "prettier": "prettier --fix src/**/*.js",
    "extract-intl": "node ./internals/translations.js",
    "serve": "nodemon server/index.js",
    "test:coverage": "jest --coverage",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "preinstall": "node ./internals/preinstall.js",
    "test:watch": "npm run test -- --watch",
    "push": "concurrently 'npm run prettier''npm run lint'  'npm run test'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run push",
      "pre-push": "npm run push"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-jest": "^22.2.2",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-check": "^5.9.0",
    "prop-types": "^15.6.2",
    "raf": "^3.4.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-intl": "^2.8.0",
    "react-test-renderer": "^16.8.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-preset-react-app": "^7.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0"
  }
}
