{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --hot --inline\" \"nodemon server/api/index.js\"",
    "lint": "eslint --fix src/**/*.js",
    "prettier": "prettier --fix src/**/*.js",
    "extract-intl": "node ./internals/translations.js",
    "start": "nodemon server/index.js",
    "test:coverage": "jest --coverage",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "preinstall": "node ./internals/preinstall.js",
    "test:watch": "npm run test -- --watch",
    "push": "concurrently \"npm run lint\" \"npm run test\"",
    "dev:api": "nodemon server/api/index.js",
    "playground": "nodemon server/playground/index.js",
    "ssr:build": "cross-env NODE_ENV=production webpack --progress",
    "ssr:server": "nodemon ./src/SSR/index.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run push",
      "pre-commit": "npm run push"
    }
  },
  "author": "Koushik Reddy Gunapati",
  "license": "ISC",
